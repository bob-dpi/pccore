  # Clocks
NET "BRDIO<0>"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P39"  ;    # 12.5 MHz clock
  # Buttons
NET "BRDIO<1>"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P13"  ;    # Button 1
NET "BRDIO<2>"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P30"  ;    # Button 2
NET "BRDIO<3>"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P69"  ;    # Button 3
  # LEDs
NET "BRDIO<4>"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P70"  ;    # LED 0
NET "BRDIO<5>"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P71"  ;    # LED 1
NET "BRDIO<6>"    IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P62"  ;    # LED 2
