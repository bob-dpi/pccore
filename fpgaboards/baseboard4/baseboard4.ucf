NET "BRDIO<11>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P65"  ;    # LED 7

  # USB interface
NET "BRDIO<12>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P89"  ;   # New data is available
NET "BRDIO<13>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P43"  ;   # Active low read data
NET "BRDIO<14>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P88"  ;   # Transmit buffer empty (not)
NET "BRDIO<15>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P47"  ;   # Write data on positive edge
NET "BRDIO<16>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P44"  ;   # USB Data 0
NET "BRDIO<17>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P41"  ;   # USB Data 1
NET "BRDIO<18>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P40"  ;   # USB Data 2
NET "BRDIO<19>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P36"  ;   # USB Data 3
NET "BRDIO<20>"   IOSTANDARD = LVCMOS33 | SLEW = SLOW | LOC = "P35"  ;   # USB Data 4
